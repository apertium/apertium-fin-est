    <rule comment="REGLA: verb-fin">
      <pattern>
        <pattern-item n="verb-fin"/>
      </pattern>
     <action>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let>
        <!-- <let><clip pos="1" side="tl" part="a_voice"/><lit-tag v="pers"/></let> -->
        <choose>
          <when><test><equal><clip pos="1" side="sl" part="a_voice"/> 
                              <lit-tag v="actv"/>
                      </equal></test>
                <let><clip pos="1" side="tl" part="a_voice"/><lit-tag v="pers"/></let>
          </when>
          <otherwise>  
                <let><clip pos="1" side="tl" part="a_voice"/><lit-tag v="impers"/></let>
          </otherwise>  
        </choose>
        <choose> 
          <when> 
            <test><equal><clip pos="1" side="sl" part="a_screeve"/><lit-tag v="ind.prs"/></equal></test>
            <let><clip pos="1" side="tl" part="a_screeve"/><lit-tag v="prs.ind"/></let>
          </when>
          <when> 
            <test><equal><clip pos="1" side="sl" part="a_screeve"/><lit-tag v="ind.prt"/></equal></test>
            <let><clip pos="1" side="tl" part="a_screeve"/><lit-tag v="prt.ind"/></let>
          </when>
        </choose>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>



